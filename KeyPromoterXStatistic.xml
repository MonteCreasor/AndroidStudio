          <Description>Debug 'app'</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'gateway-app'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'gateway-app'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'tablet-app'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'tablet-app'</Description>
          <Count>11</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
